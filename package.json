{
  "name": "estimate_backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev dotenv -e .env.dev -- nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest --forceExit",
    "test:watch": "NODE_ENV=test dotenv -e .env.test -- jest --watch --forceExit",
    "test:cov": "NODE_ENV=test dotenv -e .env.test -- jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch",
    "migrate:local": "dotenv -e .env.dev -- pnpm dlx prisma migrate dev",
    "test:int": "NODE_ENV=test dotenv -e .env.test -- jest -i --no-cache --forceExit --watch --config ./test/integrations/jest-int.json",
    "pretest:int": "pnpm run db:restart && pnpm run migrate:test",
    "db:dev:restart": "docker-compose down  && docker-compose up -d && sleep 1",
    "db:test:restart": "docker-compose -f ./docker-compose.test.yml down  && docker-compose -f ./docker-compose.test.yml up -d && sleep 1",
    "migrate:test": "dotenv -e .env.test -- pnpm dlx prisma db push --force-reset"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@langchain/community": "^0.0.25",
    "@langchain/core": "^0.1.17",
    "@langchain/google-genai": "^0.0.7",
    "@langchain/openai": "^0.0.14",
    "@langchain/pinecone": "^0.0.3",
    "@nestia/core": "^2.4.5",
    "@nestia/e2e": "^0.4.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/platform-express": "^10.0.0",
    "@pinecone-database/pinecone": "^2.0.1",
    "@prisma/client": "^5.8.1",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "d3-dsv": "2",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.3.2",
    "langchain": "^0.1.5",
    "openai": "^4.26.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typia": "^5.3.11",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@nestia/sdk": "^2.4.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.5",
    "nestia": "^5.1.2",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "prisma-markdown": "^1.0.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
